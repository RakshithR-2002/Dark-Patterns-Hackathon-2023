{
    "manifest_version": 3,
    "name": "Dark Pattern",
    "version": "1.0.0",
    "description": "Trial ext",
    "icons": {
      "48": "icon.png",
      "128": "icon.png"
    },
    "permissions": ["activeTab", "storage","tabs"],
    "background": {
      "service_worker": "background.js"
    },
  
    
   
    
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }],
    "action":{ "default_title": "Gemini AI Extension", "default_popup": "popup.html" }
  }
